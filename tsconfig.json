{
  "include": ["src/**/*"],
  "compilerOptions": {
    "target": "es2016",
    "module": "CommonJS",
    "sourceMap": false,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": false
  }
}
